{
  "name": "cypressfortest",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "open": "cypress open",
    "electron": "cypress run --spec \"cypress/e2e/Tc01_FirstScript.spec.js\"",
    "chrome": "cypress run --browser chrome --spec \"cypress/e2e/Tc01_FirstScript.spec.js\"",
    "smoke-test": "cypress run --spec \"cypress/e2e/smokeTest/*.spec.js\"",
    "test-electron-headed": "cypress run --headed",
    "test-chrome-headed": "cypress run --browser chrome --headed",
    "test-cypressForTest-Dashboard": "npx cypress run --record --key 24736850-ad74-4b13-a237-4c84749a678e",
    "scripts": "cypress run",
    "clean:reports": "rmdir /S /Q cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mochareports",
    "clean:reports-ForMac": "rm -R -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports",
    "pretest": "npm run clean:reports",
    "combine-reports": "mochawesome-merge ./cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test": "npm run scripts || npm run posttest",
    "test:chrome": "npm run chrome || npm run posttest",
    "test:electron": "npm run electron || npm run posttest"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@faker-js/faker": "^9.1.0",
    "cypress": "^13.14.1",
    "cypress-iframe": "^1.0.1",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-xpath": "^2.0.1",
    "mocha": "^10.7.3",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "neat-csv": "^5.2.0",
    "xlsx": "^0.18.5"
  }
}
